<xml id="toolbox" style="display: none">
  <category name="Elemento">
    <block type="text_element"></block>
    <block type="rectangle_element"></block>
    <block type="circle_element"></block>
    <block type="edit_element"></block>
    <block type="get_element_property"></block>
  </category>
  <category name="Campo">
    <block type="gravity_switch"></block>
    <block type="coordinate_array"></block>
  </category>
  <category name="Eventos">
    <block type="when_clicked"></block>
    <block type="when_pressed"></block>
  </category>
  <category name="Variables" custom="VARIABLE"></category>
  <category name="Condicionales">
    <block type="controls_if"></block>
    <block type="logic_compare"></block>
    <block type="logic_operation"></block>
    <block type="logic_boolean"></block>
  </category>
  <category name="Bucle">
    <block type="controls_repeat_ext">
      <value name="TIMES">
        <block type="math_number">
          <field name="NUM">10</field>
        </block>
      </value>
    </block>
  </category>
  <category name="Texto">
    <block type="text"></block>
    <block type="text_join"></block>
    <block type="text_length"></block>
    <block type="text_print"></block>
  </category>
  <category name="NÃºmeros">
    <block type="math_number"></block>
    <block type="math_arithmetic"></block>
    <block type="math_random_int">
      <value name="FROM">
        <block type="math_number">
          <field name="NUM">1</field>
        </block>
      </value>
      <value name="TO">
        <block type="math_number">
          <field name="NUM">100</field>
        </block>
      </value>
    </block>
    <block type="math_random_float"></block>
  </category>
  <category name="Otros">
    <block type="wait_for">
      <value name="seconds">
        <block type="math_number">
          <field name="NUM">1</field>
        </block>
      </value>
    </block>
    <!--<block type="console_log"></block>-->
    <!--<block type="lists_create_with"></block>-->
  </category>
</xml>
