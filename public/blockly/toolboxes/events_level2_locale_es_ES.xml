<xml id="toolbox" style="display: none">
  <category name="Tutorial">
    <block type="circle_element">
      <field name="name">bola</field>
      <mutation properties="position"></mutation>
      <value name="position_mutator">
        <block type="coordinate_array">
          <value name="xcoord">
            <block type="math_number">
              <field name="NUM">6</field>
            </block>
          </value>
          <value name="ycoord">
            <block type="math_number">
              <field name="NUM">6</field>
            </block>
          </value>
        </block>
      </value>
    </block>
    <block type="when_clicked"></block>
    <block type="text_element">
      <value name="text">
        <block type="text">
          <field name="TEXT">Hola Mundo!</field>
        </block>
      </value>
    </block>
  </category>
  <category name="Otros bloques">
    <block type="gravity_switch"></block>
    <block type="when_pressed"></block>
    <block type="rectangle_element">
      <field name="name">cuadrado</field>
    </block>
    <block type="coordinate_array">
      <value name="xcoord">
        <block type="math_number">
          <field name="NUM">6</field>
        </block>
      </value>
      <value name="ycoord">
        <block type="math_number">
          <field name="NUM">6</field>
        </block>
      </value>
    </block>
  </category>
</xml>
